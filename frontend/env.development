# 开发环境配置
NODE_ENV=development

# API基础URL - 指向本地开发服务器
VITE_API_BASE_URL=http://localhost:8000

# 应用标题
VITE_APP_TITLE=工程项目流水账管理系统

# 是否启用mock数据
VITE_USE_MOCK=false

# 是否开启调试模式
VITE_DEBUG=true

# 上传文件大小限制 (MB)
VITE_UPLOAD_SIZE_LIMIT=10

# 分页默认大小
VITE_PAGE_SIZE=20

# Token存储key
VITE_TOKEN_KEY=project-ledger-token

# 项目版本
VITE_APP_VERSION=1.0.0
